div.board-container
  div.board
    div.bname
      for n in data
        h1 #{n.name}
        - break
    div.task
      div.status.srochno
      div.opisanielg
      div.opisaniesh
      div.niz
        div.nizleft
          <div style="width: 2%; margin-right: 10px;">
            div.strelka.elements
          </div>
          div.messages
            <img src="img/comm2.png"  alt="comments" style="height: 100%" class="elements">
          div.wtime
            <img src="img/clock-solid.svg" alt="time" style="height: 80%" class="elements">
            div.txttime.elements &nbsp;&nbsp; 1
        div.nizright
          div.peopletsk.elements 6 views
    div.task
      div.status.nesrochno
      div.opisanielg
      div.opisaniesh
      div.niz
        div.nizleft
          <div style="width: 2%; margin-right: 10px;"><div class="strelka elements"></div></div>
          <div class="messages"><img src="img/comm2.png"  alt="comments" style="height: 100%" class="elements"></div>
          <div class="wtime"><img src="img/clock-solid.svg" alt="time" style="height: 80%" class="elements"><div class="txttime elements">&nbsp;&nbsp; </div></div>
        div.nizright
          div.peopletsk.elements 6 views
  include addtskbutton.pug
